<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Exolucere.ca blag</title>
 <link href="http://exolucere.ca/atom.xml" rel="self" />
 <link href="http://exolucere.ca/" />
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://exolucere.ca/</id>
 <author>
   <name>Robert Park</name>
   <email>rbpark@exolucere.ca</email>
 </author>
{% for post in site.posts %}

 <entry>
   <title>{{ post.date | date: "%F" }}</title>
   <link href="http://exolucere.ca" />
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>exolucere-{{ post.date | date_to_xmlschema }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>

{% endfor %}
</feed>
